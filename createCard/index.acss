.page-container-classname {
    height: 100%;
    background: #f1f1f1;
}

.page {
    height: 100%;
    display: flex;
    flex-direction: column;
}

.container {
    flex-grow: 1;
    overflow: hidden;
    position: relative;
    /* padding-top: 46rpx; */
}

.card__box {
    margin: 0 auto;
}

.card__boxInner {
    position: relative;
    transition: all 0.5s;
}

.card__boxInner--small {
    transform: scale(0.7);
    transform-origin: top center;
}

.draw__wapper {
    /* overflow: hidden; */
}

.draw__box {
    background: #ffffff;
    box-shadow: 0rpx 0rpx 16rpx 0rpx rgba(0, 0, 0, 0.3);
    margin: 0 auto;
    overflow: hidden;
    position: relative;
}

.beautyList {
    height: 192rpx;
    display: flex;
    align-items: center;
}

.beautyList__scroll {
    height: 96rpx;
}

.beautyList__scrollInner {
    display: flex;
    align-items: center;
    position: relative;
}

.beauty__item {
    width: 112rpx;
    height: 96rpx;
    flex-shrink: 0;
    white-space: nowrap;
    position: relative;
}

.beauty__icon {
    width: 48rpx;
    height: 48rpx;
    margin: 0 auto 8rpx;
    display: block;
}

.beauty__item--active .beauty__text {
    color: #3964f9;
}

.beauty__border {
    position: absolute;
    top: 50%;
    transform: translateY(-50%);
    left: 256rpx;
    width: 2rpx;
    background: #ddd;
    height: 96rpx;
    z-index: 1;
}

.beauty__text {
    text-align: center;
}

.footer {
    height: 320rpx;
    background: #fff;
    position: relative;
    /* align-items: center; */
}

.footer__line {
    display: flex;
    justify-content: space-between;
    padding: 0 32rpx;
    height: 128rpx;
    align-items: center;
}

.footer__btnlist {
    display: flex;
    width: 100%;
    padding: 0 40rpx;
    border-bottom: 1rpx solid #dedede;
}

.footer__content {
    height: 192rpx;
    /* padding-top: 24rpx; */
}

.footer__btnItem {
    height: 80rpx;
    line-height: 80rpx;
    width: 104rpx;
    font-size: 32rpx;
    color: #666;
}

.footer__border {
    position: absolute;
    top: 0;
    left: 0;
    display: none;
    width: 100%;
    height: 100%;
    border: 2rpx solid #3964f9;
}

.page__isIphoneX .footer {
    height: 388rpx;
}

.handler__img,
.handler__view {
    position: absolute;
    box-sizing: border-box;
}

.handler--active .handler__view {
    border: dashed 2rpx #000;
}

.handler__icon {
    display: none;
    position: absolute;
    z-index: 1;
    box-sizing: border-box;
}

.handler--active .handler__icon {
    display: block;
}

.myCanvas {
    position: fixed;
    /* left: 0; */
    /* top:  800rpx; */
    left: 19998rpx;
}

.footer__btnword {
    display: inline-block;
    position: relative;
}

.footer__word {
    position: relative;
    z-index: 2;
    font-size: 32rpx;
}

.clotheList {
    display: flex;
    padding-top: 24rpx;
    overflow-x: auto;
    overflow-y: hidden;
    -webkit-overflow-scrolling: touch;
}

.clothe__item {
    position: relative;
    width: 144rpx;
    height: 144rpx;
    margin-right: 32rpx;
    /* border: 4rpx solid #fff; */
    background: #3964f9;
}

.clothe__img {
    width: 144rpx;
    height: 144rpx;
}

.clothe__image {
    position: absolute;
    bottom: 0;
    left: 50%;
    width: 100%;
    transform: translate(-50%);
}

.clothe__vip {
    position: absolute;
    bottom: 4rpx;
    right: 4rpx;
    width: 28rpx;
    height: 28rpx;
}

.cardInfo {
    position: absolute;
    top: 10rpx;
    left: 0;
    width: 100%;
    font-size: 24rpx;
    text-align: center;
    color: #666;
}

